# Program to calculate change for rand amounts less than 50
# Stephan Jamieson

amount = int(input("amount?\n"))
print('change')

twenties = amount//20
amount = amount%20
if twenties > 0:
    print(twenties, 'R20')
tens = amount//10
amount = amount%10
if tens > 0:
    print(tens, 'R10')
fives = amount//5
amount = amount%5
if fives > 0:
    print(fives, 'R5')
twos = amount//2
amount = amount%2
if twos > 0:
    print(twos, 'R2')
if amount > 0:
    print(amount, 'R1')
