amount = int(input("What is the change amount?\n"))
print('The amounts of twenties, tens, fives, twos and ones is as follows:')

twenties = amount//20
amount = amount%20
if twenties > 0:
    print(twenties, ' x R20')
tens = amount//10
amount = amount%10
if tens > 0:
    print(tens, ' x R10')
fives = amount//5
amount = amount%5
if fives > 0:
    print(fives, ' x R5')
twos = amount//2
amount = amount%2
if twos > 0:
    print(twos, ' x R2')
if amount > 0:
    print(amount, 'x R1')
